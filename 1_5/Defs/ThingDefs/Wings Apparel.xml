<?xml version="1.0" encoding="UTF-8"?>
<Defs>
		<!--Feather Wings-->
<ThingDef ParentName="RowsApparelWings">
	<defName>Rows_Wings_Feather</defName>
	<label>Feather Wings</label>
	<description>A pair of wings that protects the wearer</description>
	<graphicData>
		<texPath>Things/RowsApparel/Wings/Feather</texPath>
		<graphicClass>Graphic_Single</graphicClass>
	</graphicData>
	<apparel>
		<wornGraphicPath>Things/RowsApparel/Wings/Feather</wornGraphicPath>
		<wornGraphicData>
				<renderUtilityAsPack>true</renderUtilityAsPack>
				<male>  <scale>(0.75,0.75)</scale></male>
				<female><scale>(0.75,0.75)</scale></female>
				<thin>  <scale>(0.7,0.7)</scale></thin>
				<hulk>  <scale>(0.9,0.9)</scale></hulk>
				<fat>   <scale>(0.9,0.9)</scale></fat>
				<east>	<offset>(-0.15,0)</offset></east>
				<west>	<offset>(0.15,0)</offset></west>
		</wornGraphicData>
	</apparel>
	<costList>
		<Cloth>90</Cloth>
		<Gold>9</Gold>
	</costList>
	<comps>
		<li Class="CompProperties_ApparelReloadable" MayRequire="Ludeon.RimWorld.Royalty">
			<ammoCountPerCharge>1</ammoCountPerCharge>
			<maxCharges>3</maxCharges>
			<ammoDef>Gold</ammoDef>
			<hotKey>Misc4</hotKey>
			<chargeNoun>fly</chargeNoun>
			<baseReloadTicks>60</baseReloadTicks>
        	<soundReload>Standard_Reload</soundReload>
        	<displayGizmoWhileUndrafted>false</displayGizmoWhileUndrafted>
		</li>
    </comps>
    <verbs>
		<li MayRequire="Ludeon.RimWorld.Royalty">
			<verbClass>Verb_jump</verbClass>
			<label>Fly to</label>
			<violent>false</violent>
			<hasStandardCommand>true</hasStandardCommand>
			<onlyManualCast>false</onlyManualCast>
			<forceNormalTimeSpeed>false</forceNormalTimeSpeed>
			<warmupTime>0.75</warmupTime>
			<requireLineOfSight>true</requireLineOfSight>
			<targetParams>
				<canTargetLocations>true</canTargetLocations>
				<canTargetPawns>true</canTargetPawns>
				<canTargetBuildings>false</canTargetBuildings>
			</targetParams>
		</li>
	</verbs>
	<statBases>
		<StuffEffectMultiplierArmor>0.4</StuffEffectMultiplierArmor>
		<StuffEffectMultiplierInsulation_Cold>0.5</StuffEffectMultiplierInsulation_Cold>
		<StuffEffectMultiplierInsulation_Heat>0.3</StuffEffectMultiplierInsulation_Heat>
		<EnergyShieldRechargeRate>0.01</EnergyShieldRechargeRate>
		<EnergyShieldEnergyMax>0.5</EnergyShieldEnergyMax>
		<JumpRange MayRequire="Ludeon.RimWorld.Royalty">10</JumpRange>
	</statBases>
</ThingDef>
	
