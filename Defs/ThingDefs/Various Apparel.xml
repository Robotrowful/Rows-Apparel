<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<!--Devotes-->
<ThingDef ParentName="RowsApparelOutfits">
	<defName>Rows_Apparel_DevoteTunic</defName>
	<label>Devote Tunic</label>
	<description>A elegent attire</description>
	<graphicData>
		<texPath>Things/RowsApparel/Clothes/Devote</texPath>
		<graphicClass>Graphic_Single</graphicClass>
		<shaderType>CutoutComplex</shaderType>
	</graphicData>
	<uiIconPath>Things/RowsApparel/Preview/DevoteTunic</uiIconPath>
	<apparel>
		<wornGraphicPath>Things/RowsApparel/Clothes/Devote</wornGraphicPath>
		<useWornGraphicMask>true</useWornGraphicMask>
		<bodyPartGroups>
			<li>Torso</li>
			<li>Neck</li>
			<li>Shoulders</li>
			<li>Arms</li>
		</bodyPartGroups>
		<layers>
			<li>OnSkin</li>
		</layers>
		<defaultOutfitTags>
			<li>Worker</li>
		</defaultOutfitTags>
	</apparel>
	<costStuffCount>80</costStuffCount>
	<statBases>
		<WorkToMake>1000</WorkToMake>
		<Mass>1</Mass>
		<StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
		<StuffEffectMultiplierInsulation_Cold>0.25</StuffEffectMultiplierInsulation_Cold>
		<StuffEffectMultiplierInsulation_Heat>0.25</StuffEffectMultiplierInsulation_Heat>
		<EquipDelay>2</EquipDelay>
	</statBases>
	<equippedStatOffsets>
		<SocialImpact>0.2</SocialImpact>
	</equippedStatOffsets>
</ThingDef>

